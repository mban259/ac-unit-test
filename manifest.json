{
  "name": "Codeforces Unit Test",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  
  "applications": {
    "gecko": {
      "id": "@acunittest"
    }
  },
  
  "permissions" : [
    "https://codeforces.com/contest/*/*/*",
    "contextMenus",
    "clipboardWrite",
    "storage"
  ],
  
  "icons" : {
    "48" : "icon.png"
  },
  
  "background" : {
    "scripts" : ["background.js"],
    "persistent": false
  },
  "content_scripts": [
        {
            "matches": [
                "https://codeforces.com/contest/*/*/*"
            ],
            "js": [
                "jquery-3.3.1.min.js",
                "content.js"
            ],
            "run_at" : "document_start"
        }
    ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
